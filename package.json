{
  "name": "youth-justice-service-finder",
  "version": "1.0.0",
  "description": "World-class service discovery system for Queensland youth justice services",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "worker": "node src/workers/index.js",
    "migrate": "node-pg-migrate",
    "seed": "node database/seeds/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.ts",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "setup:db": "node scripts/setup-database.js",
    "scrape:demo": "node scripts/run-demo-scraper.js",
    "scrape:test": "node scripts/test-scraper.js",
    "scrape:all": "node scripts/run-all-scrapers.js",
    "setup-elasticsearch": "node scripts/setup-elasticsearch.js",
    "api": "node scripts/start-api.js",
    "api:dev": "nodemon scripts/start-api.js",
    "temporal:setup": "./scripts/start-temporal.sh",
    "temporal:worker": "node src/temporal/worker.js",
    "temporal:scheduler": "node src/temporal/scheduler.js setup",
    "temporal:list": "node src/temporal/scheduler.js list",
    "temporal:trigger": "node src/temporal/scheduler.js trigger",
    "deploy": "./scripts/production/deploy.sh",
    "deploy:backup": "./scripts/production/backup.sh",
    "deploy:monitor": "./scripts/production/monitor.sh",
    "build": "tsc",
    "docker:build": "docker build -t youth-justice-finder .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@temporalio/activity": "^1.12.1",
    "@temporalio/client": "^1.12.1",
    "@temporalio/worker": "^1.12.1",
    "@temporalio/workflow": "^1.12.1",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^5.5.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "firecrawl": "^1.7.1",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "p-queue": "^8.0.1",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "string-similarity": "^4.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
